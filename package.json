{
  "name": "darkstar-monorepo",
  "version": "2.4.14-beta",
  "private": true,
  "scripts": {
    "dev": "concurrently -k -n FRONTEND,BACKEND \"pnpm:dev:frontend\" \"pnpm:dev:backend\"",
    "dev:frontend": "pnpm --prefix frontend run dev",
    "dev:backend": "bash scripts/dev-backend.sh"
  },
  "dependencies": {
    "chartjs-plugin-annotation": "^3.1.0",
    "concurrently": "^9.0.0"
  }
}